<dmx-serverconnect id="sc_event" url="/api/event/get_event"></dmx-serverconnect><!-- Wappler include head-page="layouts/la_event" fontawesome_5="cdn" bootstrap5="local" is="dmx-app" id="eventing" appConnect="local" components="{dmxBootstrap5TableGenerator:{},dmxFormatter:{}}" -->
<div class="container">
    <div class="row">
        <div class="col">
            <div class="table-responsive">
                <table class="table table-striped table-hover table-sm">
                    <thead>
                        <tr>
                            <th>Id</th>
                            <th>User uuid</th>
                            <th>Sobitia uuid</th>
                            <th>Data time start</th>
                            <th>Data time end</th>
                            <th>Vid sporta</th>
                            <th>Name sobitie</th>
                            <th>Url 1</th>
                            <th>Url 2</th>
                            <th>Status</th>
                            <th>Discription</th>
                        </tr>
                    </thead>
                    <tbody is="dmx-repeat" dmx-generator="bs5table" dmx-bind:repeat="sc_event.data.query" id="tableRepeat1">
                        <tr>
                            <td dmx-text="id"></td>
                            <td dmx-text="user_uuid"></td>
                            <td dmx-text="sobitia_uuid"></td>
                            <td dmx-text="data_time_start.formatDate('dd-MM-yyyy HH:mm')"></td>
                            <td dmx-text="data_time_end.formatDate('dd-MM-yyyy HH:mm')"></td>
                            <td dmx-text="vid_sporta"></td>
                            <td dmx-text="name_sobitie"></td>
                            <td dmx-text="url_1"></td>
                            <td dmx-text="url_2"></td>
                            <td dmx-text="status"></td>
                            <td dmx-text="discription"></td>
                        </tr>
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>

<meta name="ac:route" content="/event/eventing">