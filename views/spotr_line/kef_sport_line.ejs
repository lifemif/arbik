                        <th>Aziat uuid</th>
                    </tr>
                </thead>
                <tbody is="dmx-repeat" dmx-generator="bs5table" dmx-bind:repeat="sc_kef_sport_line.data.query" id="table_kef_sport_line">
                    <tr>
                        <td>
                            <button id="btn_kef_sport_line" class="btn btn-sm btn-outline-info w-100" dmx-on:click="modal_kef_sport_line.data_detail_kef_sport_line.select(id)" data-bs-toggle="modal" data-bs-target="#modal_kef_sport_line">Calculate</button>
