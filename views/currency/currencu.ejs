    </div>
    <div class="row">
        <div class="col">
            <div class="d-block">
                <div class="table-responsive">
                    <table class="table table-striped table-hover table-sm">
                        <thead>
                            <tr>
                                <th>Currency</th>
                                <th>Avg</th>
                            </tr>
                        </thead>
                        <tbody is="dmx-repeat" dmx-generator="bs5table" dmx-bind:repeat="sc_avg_valuta.data.query" id="tableRepeat2">
