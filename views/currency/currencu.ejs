                                <th>Facting spark</th>
                                <th>Summa svazki</th>
                                <th>Description</th>
                            </tr>
                        </thead>
                        <tbody is="dmx-repeat" dmx-generator="bs5table" dmx-bind:repeat="sc_svazka.data.query" id="tableRepeat1">
                            <tr>
                                <td dmx-text="id"></td>
                                <td dmx-text="user_uuid"></td>
                                <td dmx-text="name_strake"></td>
                                <td dmx-text="name_spark"></td>
